
<h1>Welcome, <%=current_user["username"]%></h1>


<form method="get" action="/logout">
  <button type="submit">Logout</button>
</form>

<h1>Other Users</h1>
<ul>

  <% users.each do |user| %>
      <li><%= user["username"]%></li>
  <% end %>

</ul>

<form method="get" action="/sort">
  <button type="submit">Sort</button>
</form>